
@{
    ViewBag.Title = "AdminLogin";
    Layout = "~/Views/_Layout1.cshtml";
}

    <form method="post" action="@Url.Action("AfterLogin", "Home")">
        <div class="form-group">
            <label for="exampleInputEmail1">Admin Name</label>
            <input type="text" name="name" class="form-control" id="exampleInputEmail1" placeholder="Enter Your User Name">
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Password</label>
            <input type="password" name="password" class="form-control" id="exampleInputPassword1" placeholder="Enter Your Password">
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>

